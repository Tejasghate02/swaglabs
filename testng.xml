<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sanity_Retesting_Regression_Suite">
  <test thread-count="5" name="Sanity_Retesting_Regression_Test">
	    <groups>
		  <run>
			  <include name="Sanity"></include>
			  <exclude name="Regression"></exclude>
			  <include name="Retesting"></include>
		  </run>
	  </groups>
    <classes>
      <class name="testCases.CartPageTest"/>
      <class name="testCases.LoginPageTest"/>
      <class name="testCases.InventoryPageTest"/>
    </classes>
  </test> <!-- Sanity_Retesting_Regression_Test -->
</suite> <!-- Sanity_Retesting_Regression_Suite -->
